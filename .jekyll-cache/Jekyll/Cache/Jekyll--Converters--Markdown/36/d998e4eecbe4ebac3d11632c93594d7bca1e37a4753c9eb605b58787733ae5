\hline
 1 &amp; 1 &amp; 1 &amp; 1 &amp; 2 &amp; 2 &amp; 2 &amp; 2 \\
 1 &amp; -1 &amp; -1 &amp; 1 &amp; 2 &amp; 2 &amp; -2 &amp; -2 \\
 1 &amp; -i &amp; i &amp; -1 &amp; 0 &amp; 0 &amp; 0 &amp; 0 \\
 1 &amp; i &amp; -i &amp; -1 &amp; 0 &amp; 0 &amp; 0 &amp; 0 \\
 2 &amp; 0 &amp; 0 &amp; 2 &amp; -1 &amp; -1 &amp; 0 &amp; 0 \\
\hline
\end{array}\]

